! Extensions:
! matcher
!	Enables user to click on links and open them in your browser
! resize-font
!	Enables user to resize font on fly
!	Binds: Ctrl++(increase), Ctrl+-(decrease), Ctrl+=(reset), Ctrl+?(display)
! selection-to-clipboard
!	Everything selected is moved to CLIPBOARD buffer and can be pasted with Shift+Insert
URxvt.perl-ext-common: default,matcher,resize-font,selection-to-clipboard
URxvt.url-launcher:    /usr/bin/xdg-open
URxvt.matcher.button:  1

! Enables transparency with compton compositor
URxvt.transparent:   false
URxvt.shading:       20
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0000/c000
!urxvt*background: rgba:ffff/ffff/ffff/ffff

!urxvt*transparent: true
!urxvt*shading: 10
!urxvt*tintColor: #000000
!urxvt*blurRadius: 5

!URxvt*background:                       Black
URxvt*foreground:                       White

URxvt*colorUL:                          yellow
URxvt*underlineColor:                   yellow
URxvt*cursorColor:                      Magenta

*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #ce5c00
*color7:  #babdb9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #fcaf3e
*color15: #EEEEEC

!URxvt*font: xft: Droid Sans Mono:pixelsize=15:antialias=true:hinting=true
!URxvt*font: xft: PxPlus IBM VGA8:style=Regular:pixelsize=25:antialias=true:hinting=true
!URxvt*font: xft: Inconsolata-Regular.ttf:pixelsize=20:antialias=true:hinting=true, xft: Source Code Pro:style=Regular, xft: TerminessTTF Nerd Font Mono:style=Medium:pixelsize=20
!URxvt*font: xft: Inconsolata-Regular.ttf:pixelsize=20:antialias=true:hinting=true, xft: DejaVu Sans Mono:style=Book, xft: TerminessTTF Nerd Font Mono:style=Medium:pixelsize=20
!URxvt*font: xft:Ubuntu Mono:style=Regular
!URxvt*font: xft:Unifont:style=Medium
!URxvt*font: xft: TerminessTTF Nerd Font Mono:style=Medium
URxvt*font: xft: Inconsolata-Regular.ttf:pixelsize=20:antialias=true:hinting=true, xft: Source Code Pro:style=Regular, xft: DejaVu Sans Mono:style=Book, xft: TerminessTTF Nerd Font Mono:style=Medium:pixelsize=20, xft: Unifont Upper:style=Medium
! urxvt.letterSpace:-4
! urxvt.lineSpace:-4
URxvt.cursorUnderline:1
URxvt.scrollBar: false

! Disable Crtl+Shift binding
urxvt.iso14755: false
urxvt.iso14755_52: false

urxvt*dynamicColors: on
